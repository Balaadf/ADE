{
	"name": "customerfrom_sqldb_addressfrom_adls_dataflow",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "sql_customer_dataflow_ds",
						"type": "DatasetReference"
					},
					"name": "sqldbcustomer"
				},
				{
					"dataset": {
						"referenceName": "adls_customeraddress_dataflow_ds",
						"type": "DatasetReference"
					},
					"name": "adlscustomeraddress"
				}
			],
			"sinks": [],
			"transformations": [],
			"scriptLines": [
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> sqldbcustomer",
				"source(output(",
				"          CustomerID as string,",
				"          AddressID as string,",
				"          AddressType as string,",
				"          rowguid as string,",
				"          ModifiedDate as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> adlscustomeraddress"
			]
		}
	}
}